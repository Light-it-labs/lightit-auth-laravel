{
    "name": "light-it-labs/lightit-auth-laravel",
    "description": "This is my package lightit-auth-laravel",
    "keywords": [
        "Light-it Labs",
        "laravel",
        "lightit-auth-laravel"
    ],
    "homepage": "https://github.com/light-it-labs/lightit-auth-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergio Ojeda",
            "email": "sergio@lightit.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "webgriffe/captainhook": "^2.1",
        "worksome/coding-style": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "Lightit\\Lightit\\": "src/",
            "Lightit\\Lightit\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lightit\\Lightit\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "ecs": "vendor/bin/ecs",
        "ecs:fix": "vendor/bin/ecs --fix",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=1G",
        "phpstan:baseline": "vendor/bin/phpstan analyse --memory-limit=4G --generate-baseline",
        "rector": "vendor/bin/rector process --dry-run --ansi",
        "rector:fix": "vendor/bin/rector process --ansi",
        "fixer": "composer ecs:fix && composer phpstan && composer rector:fix"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "worksome/coding-style": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lightit\\Lightit\\LightitServiceProvider"
            ],
            "aliases": {
                "Lightit": "Lightit\\Lightit\\Facades\\Lightit"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
